{
	"$schema": "https://turbo.build/schema.json",
	"extends": [
		"//"
	],
	"tasks": {
		"bundle": {
			"outputs": [
				"dist/**"
			]
		},
		"vsix": {
			"dependsOn": [
				"bundle"
			],
			"inputs": [
				"dist/**"
			],
			"outputs": [
				"../bin/**"
			]
		},
		"watch:bundle": {
			"cache": false
		}
	}
}