{
	"name": "@roo-code/evals",
	"description": "Roo Code evals.",
	"version": "0.0.0",
	"type": "module",
	"exports": "./src/index.ts",
	"scripts": {
		"lint": "eslint src --ext=ts --max-warnings=0",
		"check-types": "tsc --noEmit",
		"_test": "vitest run",
		"clean": "rimraf dist .turbo evals.db",
		"cli": "tsx src/cli/index.ts",
		"drizzle-kit": "tsx node_modules/drizzle-kit/bin.cjs",
		"db:generate": "pnpm drizzle-kit generate",
		"db:migrate": "pnpm drizzle-kit migrate",
		"db:push": "pnpm drizzle-kit push",
		"db:init": "tsx src/db/init.ts",
		"db:reset": "rm -f evals.db && pnpm db:init"
	},
	"dependencies": {
		"@roo-code/types": "workspace:^",
		"better-sqlite3": "^9.2.2",
		"cmd-ts": "^0.13.0",
		"drizzle-orm": "^0.44.1",
		"execa": "^9.6.0",
		"p-map": "^7.0.3",
		"p-queue": "^8.1.0",
		"p-wait-for": "^5.0.2",
		"ps-tree": "^1.2.0",
		"zod": "^3.25.61"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.8",
		"@types/node": "20.x",
		"@types/ps-tree": "^1.1.6",
		"drizzle-kit": "^0.31.1",
		"tsx": "^4.21.0",
		"vitest": "^3.2.3"
	}
}
